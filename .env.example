# Environment variables for MedExtract UI
# Copy this file to .env and modify values as needed

# Backend Configuration
BACKEND_URL=http://backend:8000
PYTHONPATH=/app
PYTHONUNBUFFERED=1

# Ollama Configuration
OLLAMA_BASE_URL=http://ollama:11434

# GPU Configuration (set to empty to disable GPU)
CUDA_VISIBLE_DEVICES=""

# Port Configuration
BACKEND_PORT=8000
FRONTEND_PORT=3000
OLLAMA_PORT=11434

# Development vs Production
NODE_ENV=production